    "class Fibnacci:\n",
    "    def __init__(self,num):\n",
    "        self.num = num\n",
    "        self.col_num = 0\n",
    "        self.a,self.b = 0,1\n",
    "    def __iter__(self):\n",
    "        return self\n",
    "    def __next__(self):\n",
    "        if self.col_num < self.num:\n",
    "            res = self.a\n",
